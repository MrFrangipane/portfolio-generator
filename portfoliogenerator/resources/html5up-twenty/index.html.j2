<html>
	<head>
		<title>{{ title }}</title>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
		<link rel="stylesheet" href="assets/css/main.css" />
		<noscript><link rel="stylesheet" href="assets/css/noscript.css" /></noscript>
	</head>
	<body class="index is-preload">
		<div id="page-wrapper">

			<!-- Header -->
				<header id="header" class="alt">
					<h1 id="logo"><a href="index.html">{{ title }}</a></h1>
					<nav id="nav">
						<ul>
							<li class="current"><a href="index.html">{{ index.title }}</a></li>
                            {{ menus }}
						</ul>
					</nav>
				</header>

			<!-- Banner -->
				<section id="banner">
					<!--
						".inner" is set up as an inline-block so it automatically expands
						in both directions to fit whatever's inside it. This means it won't
						automatically wrap lines, so be sure to use line breaks where
						appropriate (<br />).
					-->
					<div class="inner">
						<header>
							<h2>{{ index.title }}</h2>
						</header>
						<p>{{ index.welcome_phrase }}</p>
						<footer>
							<ul class="buttons stacked">
								<li><a href="#main" class="button fit scrolly">{{ index.main_button }}</a></li>
							</ul>
						</footer>
					</div>
				</section>

			<!-- Main -->
				<article id="main">

					<header class="special container">
						<span class="icon solid fa-{{ index.header.icon }}"></span>
						<h2>{{ index.header.title }}</h2>
						<p>{{ index.header.text }}</p>
					</header>

                    {% for article in index.articles %}
                        {{ article }}
                    {% endfor %}

				</article>

			<!-- CTA -->
				<section id="cta">

					<header>
						<h2>{{ index.footer.title }}</h2>
						<p>{{ index.footer.text }}</p>
					</header>
                    {% if index.footer.button is not none %}
					<footer>
						<ul class="buttons">
							<li><a href="{{ index.footer.link }}" class="button primary">{{ index.footer.button }}</a></li>
						</ul>
					</footer>
                    {% endif %}

				</section>

			<!-- Footer -->
				<footer id="footer">

					<ul class="icons">
                        {% for link in index.footer.links %}
                            <li><a href="{{ link.link }}" class="icon brands circle fa-{{ link.type }}"><span class="label">{{ link.type }}</span></a></li>
                        {% endfor %}
					</ul>

					<ul class="copyright">
						<li>&copy; {{ index.footer.copyright }}</li><li>Design: <a href="http://html5up.net">HTML5 UP</a></li>
					</ul>

				</footer>

		</div>

		<!-- Scripts -->
			<script src="assets/js/jquery.min.js"></script>
			<script src="assets/js/jquery.dropotron.min.js"></script>
			<script src="assets/js/jquery.scrolly.min.js"></script>
			<script src="assets/js/jquery.scrollex.min.js"></script>
			<script src="assets/js/browser.min.js"></script>
			<script src="assets/js/breakpoints.min.js"></script>
			<script src="assets/js/util.js"></script>
			<script src="assets/js/main.js"></script>

	</body>
</html>